import decode
import gleam/json

pub type Data {
  Data(
    wibble_wobble: Int,
  )
}

pub fn data_decoder() -> decode.Decoder(Data) {
  deocode.into({
    use wibble_wobble <- decode.parameter
    Data(wibble_wobble:)
  })
  |> decode.field("wibbleWobble", decode.int)
}

pub fn data_to_json(data: Data) -> json.Json {
  json.object([
    #("wibbleWobble", json.int(data.wibble_wobble)),
  ])
}