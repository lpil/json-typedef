pub fn data_decoder() -> decode.Decoder(Data) {
  deocode.into({
    use amount <- decode.parameter
    use key <- decode.parameter
    Data(amount:, key:)
  })
  |> decode.optional_field("amount", decode.int)
  |> decode.optional_field("key", decode.string)
}

