import decode
import gleam/dynamic
import gleam/option

pub fn decode(data: dynamic.Dynamic) -> decode.Decoder(option.Option(Float)) {
  decode.nullable(decode.float)
  |> decode.from(data)
}

pub fn to_json(data: option.Option(Float)) -> json.Json {
  json.nullable(data, json.float)
}